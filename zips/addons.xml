<addon id="plugin.video.youtube" name="YouTube" version="6.1.4" provider-name="bromix">
    <requires>
        <import addon="xbmc.python" version="2.20.0"/>
        <import addon="script.module.six"/>
        <import addon="script.module.requests" version="2.12.4"/>
